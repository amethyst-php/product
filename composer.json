{
    "name": "railken/amethyst-product",
    "keywords": ["laravel", "amethyst", "amethyst-product", "product"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*",
        "railken/amethyst-taxonomy": "0.1.*",
        "railken/amethyst-supplier": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "product",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\ProductServiceProvider"
            ]
        }
    }
}
